{
    "multipart": [
        {   "apply": { "model": "uselessmod:block/energy_cable/center" }},
        {   "when": { "north": "true" },
            "apply": { "model": "uselessmod:block/energy_cable/side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "uselessmod:block/energy_cable/side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "uselessmod:block/energy_cable/side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "uselessmod:block/energy_cable/side", "y": 270, "uvlock": true }
        },
		{   "when": { "up": "true" },
            "apply": { "model": "uselessmod:block/energy_cable/side", "x": 270, "uvlock": true }
        },
		{   "when": { "down": "true" },
            "apply": { "model": "uselessmod:block/energy_cable/side", "x": 90, "uvlock": true }
        }
    ]
}