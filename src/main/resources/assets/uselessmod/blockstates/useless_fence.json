		{   "when": { "south": "true" },
		    "apply": { "model": "uselessmod:block/useless_fence_side", "y": 180, "uvlock": true }
		},
		{   "when": { "west": "true" },
		    "apply": { "model": "uselessmod:block/useless_fence_side", "y": 270, "uvlock": true }
		}
	]
}
